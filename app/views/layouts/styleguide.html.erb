<% content_for :head do %>
  <%= stylesheet_link_tag 'styleguide', media: 'all', 'data-turbolinks-track' => true %>
<% end %>

<% content_for :content do %>
  <%= render 'navigation' %>
  <%= yield :main %>
<% end %>

<% content_for :scripts do %>
  <%= javascript_include_tag :styleguide %>
<% end %>

<%= render template: 'layouts/base' %>
